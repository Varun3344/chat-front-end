# Public URLs (safe to expose)
NEXT_PUBLIC_CHAT_API_BASE=https://<railway-app>.up.railway.app
NEXT_PUBLIC_CHAT_SOCKET_URL=https://<railway-app>.up.railway.app

# Optional: override base URL for server-only fetches
CHAT_API_BASE=https://<railway-app>.up.railway.app

# Server-only API keys. Never prefix with NEXT_PUBLIC_.
CHAT_API_KEY_DIRECT=<direct-send-key>
CHAT_API_KEY_DIRECT_FETCH=<direct-fetch-key>
CHAT_API_KEY_DIRECT_ATTACHMENT=<direct-attachment-key>
CHAT_API_KEY_DIRECT_DELETE=<direct-delete-key>
CHAT_API_KEY_GROUP=<group-message-key>
CHAT_API_KEY_GROUP_CREATE=<group-create-key>
CHAT_API_KEY_GROUP_MEMBER=<group-member-key>
CHAT_API_KEY_GROUP_ATTACHMENT=<group-attachment-key>
CHAT_API_KEY_GROUP_DELETE=<group-delete-key>
CHAT_API_KEY_ADMIN=<admin-key-if-needed>
